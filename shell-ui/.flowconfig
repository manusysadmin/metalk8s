[ignore]
.*/node_modules/module-deps/.*
.*/build/.*

[include]
src/.*

[libs]

[lints]
all=error
untyped-import=off
unclear-type=off
sketchy-null=off
unsafe-getters-setters=warn

[options]
esproposal.optional_chaining=enable
react.runtime=automatic

[strict]

[untyped]
.*/node_modules/react-select/.*
